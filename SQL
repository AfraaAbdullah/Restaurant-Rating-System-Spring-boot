## Create Database ##
CREATE DATABASE `cuisinerating`;

## Create User Table #
create TABLE `user` (
  `id` int(10) AUTO_INCREMENT,
  `email` varchar(50) NOT NULL UNIQUE,
  `firstname` varchar(15) NOT NULL,
  `lastname` varchar(15) NOT NULL,
  `password` varchar(12) NOT NULL,
   `contact`  bigint,
  PRIMARY KEY (`id`)
)
